bin/tester tests/vars.vera --vars --prules
--------------------------------------------------
RUL 0:|#|variables,a,b
RUL 1:||a:5
RUL 2:||a -> b
RUL 3:|a,a -> b|b,a -> b
RUL 4:|a -> b|
==================================================
bin/tester tests/vars.vera --vars --prules --force
--------------------------------------------------
RUL 0:|#|variables,a,b
RUL 1:||a:5
RUL 2:||a -> b
RUL 3:|a,a -> b|b,a -> b
RUL 4:|a -> b|
RUL 5:|b,b -> a|a,b -> a
RUL 6:|b -> a|
==================================================
bin/variables tests/vars.vera
--------------------------------------------------
|#|variables,a,b
||a:5
||a -> b
|a,a -> b|b,a -> b
|a -> b|
==================================================
bin/variables tests/vars.vera --force
--------------------------------------------------
|#|variables,a,b
||a:5
||a -> b
|a,a -> b|b,a -> b
|a -> b|
|b,b -> a|a,b -> a
|b -> a|
==================================================
bin/run tests/vars.vera --plast
--------------------------------------------------
a:5
a -> b
==================================================
bin/variables tests/vars.vera | bin/run --plast
--------------------------------------------------
b:5
==================================================
cat tests/vars.vera | bin/variables | bin/run --plast
--------------------------------------------------
b:5
==================================================
bin/run tests/vars.vera --vars --plast
--------------------------------------------------
b:5
